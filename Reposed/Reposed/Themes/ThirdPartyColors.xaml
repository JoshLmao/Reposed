<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Reposed.Themes">

    <!--Bitbucket Primary Color-->
    <Color x:Key="BitbucketPrimaryColor">#172B4D</Color>
    <SolidColorBrush x:Key="BitbucketPrimaryBrush" Color="{StaticResource BitbucketPrimaryColor}" />
    <!--GitLab primary color-->
    <Color x:Key="GitLabPrimaryColor">#FCA326</Color>
    <SolidColorBrush x:Key="GitLabPrimaryBrush" Color="{StaticResource GitLabPrimaryColor}" />
</ResourceDictionary>